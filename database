import sqlite3
import pandas as pd

#initiate database
sqlite3.connect('data)
conn = sqlite3.connect('data')
c = conn.cursor()

c.execute(''' 
              CREATE TABLE IF NOT EXISTS daily_database 
              ([count] INTEGER PRIMARY KEY, [power] FLOAT, [xStroke] FLOAT, [yStroke] FLOAT, [mode] INTEGER, [time] TEXT)
              ''')

c.execute(''' 
              CREATE TABLE IF NOT EXISTS full_database
              ([count_full] INTEGER PRIMARY KEY, [power] FLOAT, [sunset] TEXT, [sunrise] TEXT)
              ''')

conn.commit()
